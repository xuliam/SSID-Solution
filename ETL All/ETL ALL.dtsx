                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT        Sales.Customer.CustomerID, Person.Person.FirstName, Person.Person.MiddleName, Person.Person.LastName, Sales.Store.Name AS StoreName, Sales.SalesTerritory.Name AS TerritoryName, 
                         Person.CountryRegion.Name AS RegionName
